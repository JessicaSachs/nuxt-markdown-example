{
  "name": "nuxt-markdown",
  "version": "1.0.0",
  "description": "My sweet Nuxt.js project",
  "author": "Jess",
  "private": true,
  "scripts": {
    "dev": "npm run watch-styleguidist && nuxt",
    "build": "npm run watch-styleguidist && nuxt build",
    "start": "npm run watch-styleguidist && nuxt start",
    "generate": "npm run watch-styleguidist && nuxt generate",
    "watch-styleguidist": "watch 'npm run styleguidist' ./your-app &",
    "styleguidist": "vue-docgen your-app/components/**/*.vue dist/docs"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/markdownit": "^1.2.7",
    "nuxt": "^2.0.0",
    "vue-docgen-cli": "^4.8.1",
    "vue-live": "^1.5.1"
  },
  "devDependencies": {
    "@nuxtjs/vuetify": "^1.0.0",
    "watch": "^1.0.2",
    "glob": "latest"
  }
}
